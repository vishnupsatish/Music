u0bb2\u0bb5\u0bbf", "T": "\u0baa\u0bbe\u0bb2\u0bbf\u0ba9\u0bcd\u0b9a\u0bc1 \u0b95\u0bbe\u0bae\u0bbe\u0b95\u0bcd\u0bb7\u0bc0 \u0baa\u0bbe\u0bb5\u0ba9\u0bc0 <br/> \u0baa\u0bbe\u0baa \u0b9a\u0bae\u0ba9\u0bc0 \u0b85\u0bae\u0bcd\u0baa"}, {"M": "\u0b85\u0ba9\u0bc1\u0baa\u0bb2\u0bcd\u0bb2\u0bb5\u0bbf", "T": "\u0b9a\u0bbe\u0bb2\u0bbe \u0baa\u0bb9\u0bc1 \u0bb5\u0bbf\u0ba4\u0bae\u0bc1\u0b95\u0bbe \u0ba8\u0bbf\u0ba9\u0bcd\u0ba9\u0bc1 <br/> \u0b9a\u0ba4\u0bbe \u0bb5\u0bc7\u0b9f\u0bc1\u0b95\u0bca\u0ba9\u0bc7\u0b9f\u0bbf \u0ba8\u0bbe\u0baf\u0ba8\u0bcd\u0ba4\u0bcd(\u0b8f)\u0bb2 <br/> \u0b88 \u0bb2\u0bbe\u0b95\u0bc1 \u0b9c\u0bc7\u0b9a\u0bc7\u0bb5\u0bc1 \u0bb5\u0bc6\u0ba4 <br/> \u0bb9\u0bb0\u0bbf\u0ba9\u0bcd\u0b9a\u0bb5\u0bc7 \u0bb5\u0bc7\u0b95\u0bae\u0bc7 \u0ba8\u0ba9\u0bcd\u0ba9\u0bc1 (\u0baa\u0bbe\u0bb2\u0bbf\u0ba9\u0bcd\u0b9a\u0bc1)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 1", "T": "\u0bb8\u0bcd\u0bb5\u0bbe\u0ba8\u0bcd\u0ba4\u0bae\u0bcd\u0baa\u0bc1\u0bb2\u0bcb\u0ba9 \u0ba8\u0bbf\u0ba9\u0bcd\u0ba9\u0bc7 <br/> \u0ba4\u0bb2\u0b9a\u0bbf\u0ba9 \u0b9a\u0bc1\u0b9c\u0ba9\u0bc1\u0bb2\u0b95\u0bcd(\u0b8e)\u0bb2\u0bcd\u0bb2\u0ba9\u0bcd(\u0b8f) \u0bb5\u0bc7\u0bb3 <br/> \u0b9a\u0ba8\u0bcd\u0ba4\u0bcb\u0bb7\u0bae\u0bc1\u0bb2\u0bcd(\u0b92)\u0b9a\u0b95\u0bc7\u0bb5\u0bcd(\u0b85)\u0ba8\u0bbf \u0ba8\u0bc0\u0bb5\u0bc1 <br/> \u0bae\u0ba9\u0bcb\u0bb0\u0ba4 \u0baa\u0bb2 \u0ba4\u0bbe\u0baf\u0bbf\u0ba9\u0bbf\u0bb5\u0bcd(\u0b85)\u0ba8\u0bbf <br/> \u0b95\u0bbe\u0ba8\u0bcd\u0ba4\u0bae\u0b95\u0bc1 \u0baa\u0bc7\u0bb0\u0bc1 \u0baa\u0bca\u0ba8\u0bcd\u0ba4\u0bbf\u0ba4\u0bbf\u0bb5\u0bbf <br/> \u0b95\u0bbe\u0bb0\u0bc1\u0ba3\u0bcd\u0baf \u0bae\u0bc2\u0bb0\u0bcd\u0ba4\u0bbf\u0bb5\u0bc8 \u0b9c\u0b95\u0bae\u0bc1 <br/> \u0b95\u0bbe\u0baa\u0bbe\u0b9f\u0bbf\u0ba9 \u0ba4\u0bb2\u0bcd\u0bb2\u0bbf \u0b95\u0ba4\u0bbe \u0ba8\u0bc7\u0ba9\u0bc1 <br/> \u0ba8\u0bc0\u0ba4\u0bc1 \u0baa\u0bbf\u0b9f\u0bcd\u0b9f\u0ba9\u0bc1 \u0bb2\u0bbe\u0bb2\u0bbf\u0ba9\u0bcd\u0b9a\u0bbf (\u0baa\u0bbe\u0bb2\u0bbf\u0ba9\u0bcd\u0b9a\u0bc1)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0b88 \u0bae\u0bc2\u0bb0\u0bcd\u0ba4\u0bbf(\u0baf\u0bcd)\u0b87\u0ba8\u0bcd\u0ba4 \u0ba4\u0bc7\u0b9c\u0bcb-\u0bae\u0baf\u0bae\u0bc8- <br/> (\u0baf\u0bcd)\u0b87\u0b9f\u0bc1 \u0bb5\u0bb2\u0bc6 \u0b95\u0bc0\u0bb0\u0bcd\u0ba4\u0bbf \u0bb5\u0bbf\u0bb8\u0bcd\u0baa\u0bc2\u0bb0\u0bcd\u0ba4\u0bbf(\u0ba8\u0bcd)- <br/> \u0b87\u0b9f\u0bcd\u0bb2\u0ba9\u0bc1 \u0b95\u0bc1\u0ba3 \u0bae\u0bc2\u0bb0\u0bcd\u0ba4\u0bbf \u0ba4\u0bcd\u0bb0\u0bbf-\u0bb2\u0bcb\u0b95\u0bae\u0bc1\u0bb2\u0bcb <br/> \u0b9c\u0bc2\u0b9a\u0bbf\u0ba9(\u0baf\u0bcd)\u0b8e\u0ba8\u0bcd\u0ba4\u0bcd(\u0b90)\u0ba8 \u0b95\u0bb2\u0ba4\u0bbe <br/> \u0b8f\u0bae\u0bcb \u0ba4\u0bca\u0bb2\u0bbf \u0ba8\u0bcb\u0bae\u0bc1 \u0ba8\u0bcb\u0b9a\u0bbf\u0ba4\u0bbf\u0ba9\u0bcb <br/> \u0ba8\u0bc0 \u0baa\u0bbe\u0ba4 \u0baa\u0ba4\u0bcd\u0bae \u0ba4\u0bb0\u0bcd\u0b9a\u0ba9\u0bae\u0bc1 <br/> \u0bb5\u0bc7\u0bae\u0bbe\u0bb0\u0bc1 \u0bb2\u0baa\u0bbf\u0ba9\u0bcd\u0b9a\u0bbf \u0b95\u0bcd\u0bb0\u0bc1\u0ba4\u0bcd(\u0b86)\u0bb0\u0bcd\u0ba4\u0bc1\u0b9f\u0ba9\u0bcd(\u0b90)\u0ba4\u0bbf <br/> \u0ba8\u0bbe \u0bae\u0ba9\u0bb5\u0bbf\u0ba9\u0bcd(\u0b86)\u0bb2\u0b95\u0bbf\u0ba9\u0bcd\u0b9a\u0bbf (\u0baa\u0bbe\u0bb2\u0bbf\u0ba9\u0bcd\u0b9a\u0bc1)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0bb0\u0bbe\u0b9c\u0bcd(\u0b86)\u0ba4\u0bbf \u0bb0\u0bbe\u0b9c\u0ba9\u0bcd-\u0bae\u0b95\u0bc1\u0b9f\u0bc0 <br/> \u0ba4\u0b9f \u0bae\u0ba3\u0bbf \u0bb0\u0bbe\u0b9c \u0baa\u0bbe\u0ba4\u0bbe <br/> \u0ba8\u0bc7 \u0b9a\u0bbe\u0bb2 \u0ba8\u0bbf\u0b9c \u0b9a\u0ba9\u0bcd\u0ba9\u0bbf\u0ba4\u0bbf\u0ba9\u0bbf \u0b95\u0bcb\u0bb0\u0bbf <br/> \u0b9a\u0bae\u0bb8\u0bcd\u0ba4 \u0b9c\u0ba9\u0bc1\u0bb2\u0b95\u0bcd(\u0b8e)\u0bb2\u0bcd\u0bb2 \u0bb5\u0bb0\u0ba4\u0bbe <br/> \u0bb0\u0bbe\u0b9c \u0bae\u0bc1\u0b95\u0bc0 \u0bb6\u0bcd\u0baf\u0bbe\u0bae \u0b95\u0bcd\u0bb0\u0bc1\u0bb7\u0bcd\u0ba3 \u0ba8\u0bc1\u0ba4\u0bbe <br/> \u0b95\u0bbe\u0ba9\u0bcd\u0b9a\u0bc0 \u0baa\u0bc1\u0bb0\u0bcd(\u0b8f)\u0bb6\u0bcd\u0bb5\u0bb0\u0bc0 \u0bb5\u0bbf\u0b95\u0b9a <br/> \u0bb0\u0bbe\u0b9c\u0bc0\u0bb5 \u0ba4\u0bb3\u0bcd(\u0b86)\u0b95\u0bcd\u0bb7\u0bc0 \u0b9c\u0b95\u0ba4\u0bcd-\u0b9a\u0bbe\u0b95\u0bcd\u0bb7\u0bc0 <br/> \u0b93 \u0baa\u0bcd\u0bb0\u0b9a\u0ba9\u0bcd\u0ba9 \u0baa\u0bb0\u0bbe \u0b9a\u0b95\u0bcd\u0ba4\u0bc0 (\u0baa\u0bbe\u0bb2\u0bbf\u0ba9\u0bcd\u0b9a\u0bc1)"}, {"M": "\u0bb8\u0bcd\u0bb5\u0bb0 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0b95\u0ba9\u0b95 \u0b95\u0bbf\u0bb0\u0bbf \u0b9a\u0ba4\u0ba9 \u0bb2\u0bb2\u0bbf\u0ba4 \u0ba8\u0bbf\u0ba9\u0bc1 \u0baa\u0b9c\u0ba9 <br/> \u0b9a\u0ba8\u0bcd\u0ba4\u0ba4\u0bae\u0bc1 \u0b9a\u0bc7\u0baf\u0ba9\u0bbf \u0b9c\u0b9f\u0bc1\u0b9f\u0ba9\u0bc1 <br/> \u0bb5\u0bbf\u0ba9\u0bc1\u0bae\u0bc1 \u0ba8\u0bbf\u0b95\u0bbf\u0bb2 \u0baa\u0bc1\u0bb5\u0ba9 \u0b9c\u0ba9\u0ba9\u0bbf\u0bb5\u0bbf(\u0baf\u0bcd)\u0b87\u0baa\u0bc1\u0b9f\u0bc1 <br/> \u0bae\u0bbe \u0ba4\u0bc1\u0bb0\u0bbf\u0ba4\u0bae\u0bc1 \u0ba4\u0bc0\u0bb0\u0bcd\u0b9a\u0bbf \u0bb5\u0bb0\u0bbe\u0bb2\u0bcd(\u0b87)\u0b9a\u0bcd\u0b9a\u0bbf (\u0baa\u0bbe\u0bb2\u0bbf\u0ba9\u0bcd\u0b9a\u0bc1)"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2a\u0c3e\u0c32\u0c3f\u0c28\u0c4d\u0c1a\u0c41 \u0c15\u0c3e\u0c2e\u0c3e\u0c15\u0c4d\u0c37\u0c40 \u0c2a\u0c3e\u0c35\u0c28\u0c40 <br/> \u0c2a\u0c3e\u0c2a \u0c36\u0c2e\u0c28\u0c40 \u0c05\u0c2e\u0c4d\u0c2c"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c1a\u0c3e\u0c32\u0c3e \u0c2c\u0c39\u0c41 \u0c35\u0c3f\u0c27\u0c2e\u0c41\u0c17\u0c3e \u0c28\u0c3f\u0c28\u0c4d\u0c28\u0c41 <br/> \u0c38\u0c26\u0c3e \u0c35\u0c47\u0c21\u0c41\u0c15\u0c4a\u0c28\u0c47\u0c21\u0c3f \u0c28\u0c3e\u0c2f\u0c28\u0c4d\u0c26\u0c4d(\u0c0f)\u0c32 <br/> \u0c08 \u0c32\u0c3e\u0c17\u0c41 \u0c1c\u0c47\u0c38\u0c47\u0c35\u0c41 \u0c35\u0c46\u0c24 <br/> \u0c39\u0c30\u0c3f\u0c28\u0c4d\u0c1a\u0c35\u0c47 \u0c35\u0c47\u0c17\u0c2e\u0c47 \u0c28\u0c28\u0c4d\u0c28\u0c41 (\u0c2a\u0c3e\u0c32\u0c3f\u0c28\u0c4d\u0c1a\u0c41)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c38\u0c4d\u0c35\u0c3e\u0c28\u0c4d\u0c24\u0c2e\u0c4d\u0c2c\u0c41\u0c32\u0c4b\u0c28 \u0c28\u0c3f\u0c28\u0c4d\u0c28\u0c47 <br/> \u0c26\u0c32\u0c1a\u0c3f\u0c28 \u0c38\u0c41\u0c1c\u0c28\u0c41\u0c32\u0c15\u0c4d(\u0c0e)\u0c32\u0c4d\u0c32\u0c28\u0c4d(\u0c0f) \u0c35\u0c47\u0c33 <br/> \u0c38\u0c28\u0c4d\u0c24\u0c4b\u0c37\u0c2e\u0c41\u0c32\u0c4d(\u0c12)\u0c38\u0c17\u0c47\u0c35\u0c4d(\u0c05)\u0c28\u0c3f \u0c28\u0c40\u0c35\u0c41 <br/> \u0c2e\u0c28\u0c4b\u0c30\u0c25 \u0c2b\u0c32 \u0c26\u0c3e\u0c2f\u0c3f\u0c28\u0c3f\u0c35\u0c4d(\u0c05)\u0c28\u0c3f <br/> \u0c15\u0c3e\u0c28\u0c4d\u0c24\u0c2e\u0c17\u0c41 \u0c2a\u0c47\u0c30\u0c41 \u0c2a\u0c4a\u0c28\u0c4d\u0c26\u0c3f\u0c24\u0c3f\u0c35\u0c3f <br/> \u0c15\u0c3e\u0c30\u0c41\u0c23\u0c4d\u0c2f \u0c2e\u0c42\u0c30\u0c4d\u0c24\u0c3f\u0c35\u0c48 \u0c1c\u0c17\u0c2e\u0c41 <br/> \u0c15\u0c3e\u0c2a\u0c3e\u0c21\u0c3f\u0c28 \u0c24\u0c32\u0c4d\u0c32\u0c3f \u0c17\u0c26\u0c3e \u0c28\u0c47\u0c28\u0c41 <br/> \u0c28\u0c40\u0c26\u0c41 \u0c2c\u0c3f\u0c21\u0c4d\u0c21\u0c28\u0c41 \u0c32\u0c3e\u0c32\u0c3f\u0c28\u0c4d\u0c1a\u0c3f (\u0c2a\u0c3e\u0c32\u0c3f\u0c28\u0c4d\u0c1a\u0c41)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c08 \u0c2e\u0c42\u0c30\u0c4d\u0c24\u0c3f(\u0c2f\u0c4d)\u0c07\u0c28\u0c4d\u0c24 \u0c24\u0c47\u0c1c\u0c4b-\u0c2e\u0c2f\u0c2e\u0c48- <br/> (\u0c2f\u0c4d)\u0c07\u0c1f\u0c41 \u0c35\u0c32\u0c46 \u0c15\u0c40\u0c30\u0c4d\u0c24\u0c3f \u0c35\u0c3f\u0c38\u0c4d\u0c2b\u0c42\u0c30\u0c4d\u0c24\u0c3f(\u0c28\u0c4d)- <br/> \u0c07\u0c1f\u0c4d\u0c32\u0c28\u0c41 \u0c17\u0c41\u0c23 \u0c2e\u0c42\u0c30\u0c4d\u0c24\u0c3f \u0c24\u0c4d\u0c30\u0c3f-\u0c32\u0c4b\u0c15\u0c2e\u0c41\u0c32\u0c4b <br/> \u0c1c\u0c42\u0c1a\u0c3f\u0c28(\u0c2f\u0c4d)\u0c0e\u0c28\u0c4d\u0c26\u0c4d(\u0c10)\u0c28 \u0c17\u0c32\u0c26\u0c3e <br/> \u0c0f\u0c2e\u0c4b \u0c24\u0c4a\u0c32\u0c3f \u0c28\u0c4b\u0c2e\u0c41 \u0c28\u0c4b\u0c1a\u0c3f\u0c24\u0c3f\u0c28\u0c4b <br/> \u0c28\u0c40 \u0c2a\u0c3e\u0c26 \u0c2a\u0c26\u0c4d\u0c2e \u0c26\u0c30\u0c4d\u0c36\u0c28\u0c2e\u0c41 <br/> \u0c35\u0c47\u0c2e\u0c3e\u0c30\u0c41 \u0c32\u0c2d\u0c3f\u0c28\u0c4d\u0c1a\u0c3f \u0c15\u0c43\u0c24\u0c4d(\u0c06)\u0c30\u0c4d\u0c25\u0c41\u0c21\u0c28\u0c4d(\u0c10)\u0c24\u0c3f <br/> \u0c28\u0c3e \u0c2e\u0c28\u0c35\u0c3f\u0c28\u0c4d(\u0c06)\u0c32\u0c15\u0c3f\u0c28\u0c4d\u0c1a\u0c3f (\u0c2a\u0c3e\u0c32\u0c3f\u0c28\u0c4d\u0c1a\u0c41)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c30\u0c3e\u0c1c\u0c4d(\u0c06)\u0c27\u0c3f \u0c30\u0c3e\u0c1c\u0c28\u0c4d-\u0c2e\u0c15\u0c41\u0c1f\u0c40 <br/> \u0c24\u0c1f \u0c2e\u0c23\u0c3f \u0c30\u0c3e\u0c1c \u0c2a\u0c3e\u0c26\u0c3e <br/> \u0c28\u0c47 \u0c1a\u0c3e\u0c32 \u0c28\u0c3f\u0c1c \u0c38\u0c28\u0c4d\u0c28\u0c3f\u0c27\u0c3f\u0c28\u0c3f \u0c15\u0c4b\u0c30\u0c3f <br/> \u0c38\u0c2e\u0c38\u0c4d\u0c24 \u0c1c\u0c28\u0c41\u0c32\u0c15\u0c4d(\u0c0e)\u0c32\u0c4d\u0c32 \u0c35\u0c30\u0c26\u0c3e <br/> \u0c30\u0c3e\u0c1c \u0c2e\u0c41\u0c16\u0c40 \u0c36\u0c4d\u0c2f\u0c3e\u0c2e \u0c15\u0c43\u0c37\u0c4d\u0c23 \u0c28\u0c41\u0c24\u0c3e <br/> \u0c15\u0c3e\u0c28\u0c4d\u0c1a\u0c40 \u0c2a\u0c41\u0c30\u0c4d(\u0c0f)\u0c36\u0c4d\u0c35\u0c30\u0c40 \u0c35\u0c3f\u0c15\u0c38 <br/> \u0c30\u0c3e\u0c1c\u0c40\u0c35 \u0c26\u0c33\u0c4d(\u0c06)\u0c15\u0c4d\u0c37\u0c40 \u0c1c\u0c17\u0c24\u0c4d-\u0c38\u0c3e\u0c15\u0c4d\u0c37\u0c40 <br/> \u0c13 \u0c2a\u0c4d\u0c30\u0c38\u0c28\u0c4d\u0c28 \u0c2a\u0c30\u0c3e \u0c36\u0c15\u0c4d\u0c24\u0c40 (\u0c2a\u0c3e\u0c32\u0c3f\u0c28\u0c4d\u0c1a\u0c41)"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c15\u0c28\u0c15 \u0c17\u0c3f\u0c30\u0c3f \u0c38\u0c26\u0c28 \u0c32\u0c32\u0c3f\u0c24 \u0c28\u0c3f\u0c28\u0c41 \u0c2d\u0c1c\u0c28 <br/> \u0c38\u0c28\u0c4d\u0c24\u0c24\u0c2e\u0c41 \u0c38\u0c47\u0c2f\u0c28\u0c3f \u0c1c\u0c21\u0c41\u0c21\u0c28\u0c41 <br/> \u0c35\u0c3f\u0c28\u0c41\u0c2e\u0c41 \u0c28\u0c3f\u0c16\u0c3f\u0c32 \u0c2d\u0c41\u0c35\u0c28 \u0c1c\u0c28\u0c28\u0c3f\u0c35\u0c3f(\u0c2f\u0c4d)\u0c07\u0c2a\u0c41\u0c21\u0c41 <br/> \u0c2e\u0c3e \u0c26\u0c41\u0c30\u0c3f\u0c24\u0c2e\u0c41 \u0c26\u0c40\u0c30\u0c4d\u0c1a\u0c3f \u0c35\u0c30\u0c3e\u0c32\u0c4d(\u0c07)\u0c1a\u0c4d\u0c1a\u0c3f (\u0c2a\u0c3e\u0c32\u0c3f\u0c28\u0c4d\u0c1a\u0c41)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0caa\u0cbe\u0cb2\u0cbf\u0ca8\u0ccd\u0c9a\u0cc1 \u0c95\u0cbe\u0cae\u0cbe\u0c95\u0ccd\u0cb7\u0cc0 \u0caa\u0cbe\u0cb5\u0ca8\u0cc0 <br/> \u0caa\u0cbe\u0caa \u0cb6\u0cae\u0ca8\u0cc0 \u0c85\u0cae\u0ccd\u0cac"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c9a\u0cbe\u0cb2\u0cbe \u0cac\u0cb9\u0cc1 \u0cb5\u0cbf\u0ca7\u0cae\u0cc1\u0c97\u0cbe \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0cc1 <br/> \u0cb8\u0ca6\u0cbe \u0cb5\u0cc7\u0ca1\u0cc1\u0c95\u0cca\u0ca8\u0cc7\u0ca1\u0cbf \u0ca8\u0cbe\u0caf\u0ca8\u0ccd\u0ca6\u0ccd(\u0c8f)\u0cb2 <br/> \u0c88 \u0cb2\u0cbe\u0c97\u0cc1 \u0c9c\u0cc7\u0cb8\u0cc7\u0cb5\u0cc1 \u0cb5\u0cc6\u0ca4 <br/> \u0cb9\u0cb0\u0cbf\u0ca8\u0ccd\u0c9a\u0cb5\u0cc7 \u0cb5\u0cc7\u0c97\u0cae\u0cc7 \u0ca8\u0ca8\u0ccd\u0ca8\u0cc1 (\u0caa\u0cbe\u0cb2\u0cbf\u0ca8\u0ccd\u0c9a\u0cc1)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0cb8\u0ccd\u0cb5\u0cbe\u0ca8\u0ccd\u0ca4\u0cae\u0ccd\u0cac\u0cc1\u0cb2\u0ccb\u0ca8 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0cc7 <br/> \u0ca6\u0cb2\u0c9a\u0cbf\u0ca8 \u0cb8\u0cc1\u0c9c\u0ca8\u0cc1\u0cb2\u0c95\u0ccd(\u0c8e)\u0cb2\u0ccd\u0cb2\u0ca8\u0ccd(\u0c8f) \u0cb5\u0cc7\u0cb3 <br/> \u0cb8\u0ca8\u0ccd\u0ca4\u0ccb\u0cb7\u0cae\u0cc1\u0cb2\u0ccd(\u0c92)\u0cb8\u0c97\u0cc7\u0cb5\u0ccd(\u0c85)\u0ca8\u0cbf \u0ca8\u0cc0\u0cb5\u0cc1 <br/> \u0cae\u0ca8\u0ccb\u0cb0\u0ca5 \u0cab\u0cb2 \u0ca6\u0cbe\u0caf\u0cbf\u0ca8\u0cbf\u0cb5\u0ccd(\u0c85)\u0ca8\u0cbf <br/> \u0c95\u0cbe\u0ca8\u0ccd\u0ca4\u0cae\u0c97\u0cc1 \u0caa\u0cc7\u0cb0\u0cc1 \u0caa\u0cca\u0ca8\u0ccd\u0ca6\u0cbf\u0ca4\u0cbf\u0cb5\u0cbf <br/> \u0c95\u0cbe\u0cb0\u0cc1\u0ca3\u0ccd\u0caf \u0cae\u0cc2\u0cb0\u0ccd\u0ca4\u0cbf\u0cb5\u0cc8 \u0c9c\u0c97\u0cae\u0cc1 <br/> \u0c95\u0cbe\u0caa\u0cbe\u0ca1\u0cbf\u0ca8 \u0ca4\u0cb2\u0ccd\u0cb2\u0cbf \u0c97\u0ca6\u0cbe \u0ca8\u0cc7\u0ca8\u0cc1 <br/> \u0ca8\u0cc0\u0ca6\u0cc1 \u0cac\u0cbf\u0ca1\u0ccd\u0ca1\u0ca8\u0cc1 \u0cb2\u0cbe\u0cb2\u0cbf\u0ca8\u0ccd\u0c9a\u0cbf (\u0caa\u0cbe\u0cb2\u0cbf\u0ca8\u0ccd\u0c9a\u0cc1)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c88 \u0cae\u0cc2\u0cb0\u0ccd\u0ca4\u0cbf(\u0caf\u0ccd)\u0c87\u0ca8\u0ccd\u0ca4 \u0ca4\u0cc7\u0c9c\u0ccb-\u0cae\u0caf\u0cae\u0cc8- <br/> (\u0caf\u0ccd)\u0c87\u0c9f\u0cc1 \u0cb5\u0cb2\u0cc6 \u0c95\u0cc0\u0cb0\u0ccd\u0ca4\u0cbf \u0cb5\u0cbf\u0cb8\u0ccd\u0cab\u0cc2\u0cb0\u0ccd\u0ca4\u0cbf(\u0ca8\u0ccd)- <br/> \u0c87\u0c9f\u0ccd\u0cb2\u0ca8\u0cc1 \u0c97\u0cc1\u0ca3 \u0cae\u0cc2\u0cb0\u0ccd\u0ca4\u0cbf \u0ca4\u0ccd\u0cb0\u0cbf-\u0cb2\u0ccb\u0c95\u0cae\u0cc1\u0cb2\u0ccb <br/> \u0c9c\u0cc2\u0c9a\u0cbf\u0ca8(\u0caf\u0ccd)\u0c8e\u0ca8\u0ccd\u0ca6\u0ccd(\u0c90)\u0ca8 \u0c97\u0cb2\u0ca6\u0cbe <br/> \u0c8f\u0cae\u0ccb \u0ca4\u0cca\u0cb2\u0cbf \u0ca8\u0ccb\u0cae\u0cc1 \u0ca8\u0ccb\u0c9a\u0cbf\u0ca4\u0cbf\u0ca8\u0ccb <br/> \u0ca8\u0cc0 \u0caa\u0cbe\u0ca6 \u0caa\u0ca6\u0ccd\u0cae \u0ca6\u0cb0\u0ccd\u0cb6\u0ca8\u0cae\u0cc1 <br/> \u0cb5\u0cc7\u0cae\u0cbe\u0cb0\u0cc1 \u0cb2\u0cad\u0cbf\u0ca8\u0ccd\u0c9a\u0cbf \u0c95\u0cc3\u0ca4\u0ccd(\u0c86)\u0cb0\u0ccd\u0ca5\u0cc1\u0ca1\u0ca8\u0ccd(\u0c90)\u0ca4\u0cbf <br/> \u0ca8\u0cbe \u0cae\u0ca8\u0cb5\u0cbf\u0ca8\u0ccd(\u0c86)\u0cb2\u0c95\u0cbf\u0ca8\u0ccd\u0c9a\u0cbf (\u0caa\u0cbe\u0cb2\u0cbf\u0ca8\u0ccd\u0c9a\u0cc1)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cb0\u0cbe\u0c9c\u0ccd(\u0c86)\u0ca7\u0cbf \u0cb0\u0cbe\u0c9c\u0ca8\u0ccd-\u0cae\u0c95\u0cc1\u0c9f\u0cc0 <br/> \u0ca4\u0c9f \u0cae\u0ca3\u0cbf \u0cb0\u0cbe\u0c9c \u0caa\u0cbe\u0ca6\u0cbe <br/> \u0ca8\u0cc7 \u0c9a\u0cbe\u0cb2 \u0ca8\u0cbf\u0c9c \u0cb8\u0ca8\u0ccd\u0ca8\u0cbf\u0ca7\u0cbf\u0ca8\u0cbf \u0c95\u0ccb\u0cb0\u0cbf <br/> \u0cb8\u0cae\u0cb8\u0ccd\u0ca4 \u0c9c\u0ca8\u0cc1\u0cb2\u0c95\u0ccd(\u0c8e)\u0cb2\u0ccd\u0cb2 \u0cb5\u0cb0\u0ca6\u0cbe <br/> \u0cb0\u0cbe\u0c9c \u0cae\u0cc1\u0c96\u0cc0 \u0cb6\u0ccd\u0caf\u0cbe\u0cae \u0c95\u0cc3\u0cb7\u0ccd\u0ca3 \u0ca8\u0cc1\u0ca4\u0cbe <br/> \u0c95\u0cbe\u0ca8\u0ccd\u0c9a\u0cc0 \u0caa\u0cc1\u0cb0\u0ccd(\u0c8f)\u0cb6\u0ccd\u0cb5\u0cb0\u0cc0 \u0cb5\u0cbf\u0c95\u0cb8 <br/> \u0cb0\u0cbe\u0c9c\u0cc0\u0cb5 \u0ca6\u0cb3\u0ccd(\u0c86)\u0c95\u0ccd\u0cb7\u0cc0 \u0c9c\u0c97\u0ca4\u0ccd-\u0cb8\u0cbe\u0c95\u0ccd\u0cb7\u0cc0 <br/> \u0c93 \u0caa\u0ccd\u0cb0\u0cb8\u0ca8\u0ccd\u0ca8 \u0caa\u0cb0\u0cbe \u0cb6\u0c95\u0ccd\u0ca4\u0cc0 (\u0caa\u0cbe\u0cb2\u0cbf\u0ca8\u0ccd\u0c9a\u0cc1)"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0c95\u0ca8\u0c95 \u0c97\u0cbf\u0cb0\u0cbf \u0cb8\u0ca6\u0ca8 \u0cb2\u0cb2\u0cbf\u0ca4 \u0ca8\u0cbf\u0ca8\u0cc1 \u0cad\u0c9c\u0ca8 <br/> \u0cb8\u0ca8\u0ccd\u0ca4\u0ca4\u0cae\u0cc1 \u0cb8\u0cc7\u0caf\u0ca8\u0cbf \u0c9c\u0ca1\u0cc1\u0ca1\u0ca8\u0cc1 <br/> \u0cb5\u0cbf\u0ca8\u0cc1\u0cae\u0cc1 \u0ca8\u0cbf\u0c96\u0cbf\u0cb2 \u0cad\u0cc1\u0cb5\u0ca8 \u0c9c\u0ca8\u0ca8\u0cbf\u0cb5\u0cbf(\u0caf\u0ccd)\u0c87\u0caa\u0cc1\u0ca1\u0cc1 <br/> \u0cae\u0cbe \u0ca6\u0cc1\u0cb0\u0cbf\u0ca4\u0cae\u0cc1 \u0ca6\u0cc0\u0cb0\u0ccd\u0c9a\u0cbf \u0cb5\u0cb0\u0cbe\u0cb2\u0ccd(\u0c87)\u0c9a\u0ccd\u0c9a\u0cbf (\u0caa\u0cbe\u0cb2\u0cbf\u0ca8\u0ccd\u0c9a\u0cc1)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2a\u0d3e\u0d32\u0d3f\u0d28\u0d4d\u0d1a\u0d41 \u0d15\u0d3e\u0d2e\u0d3e\u0d15\u0d4d\u0d37\u0d40 \u0d2a\u0d3e\u0d35\u0d28\u0d40 <br/> \u0d2a\u0d3e\u0d2a \u0d36\u0d2e\u0d28\u0d40 \u0d05\u0d2e\u0d4d\u0d2c"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d1a\u0d3e\u0d32\u0d3e \u0d2c\u0d39\u0d41 \u0d35\u0d3f\u0d27\u0d2e\u0d41\u0d17\u0d3e \u0d28\u0d3f\u0d28\u0d4d\u0d28\u0d41 <br/> \u0d38\u0d26\u0d3e \u0d35\u0d47\u0d21\u0d41\u0d15\u0d4a\u0d28\u0d47\u0d21\u0d3f \u0d28\u0d3e\u0d2f\u0d28\u0d4d\u0d26\u0d4d(\u0d0f)\u0d32 <br/> \u0d08 \u0d32\u0d3e\u0d17\u0d41 \u0d1c\u0d47\u0d38\u0d47\u0d35\u0d41 \u0d35\u0d46\u0d24 <br/> \u0d39\u0d30\u0d3f\u0d28\u0d4d\u0d1a\u0d35\u0d47 \u0d35\u0d47\u0d17\u0d2e\u0d47 \u0d28\u0d28\u0d4d\u0d28\u0d41 (\u0d2a\u0d3e\u0d32\u0d3f\u0d28\u0d4d\u0d1a\u0d41)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d38\u0d4d\u0d35\u0d3e\u0d28\u0d4d\u0d24\u0d2e\u0d4d\u0d2c\u0d41\u0d32\u0d4b\u0d28 \u0d28\u0d3f\u0d28\u0d4d\u0d28\u0d47 <br/> \u0d26\u0d32\u0d1a\u0d3f\u0d28 \u0d38\u0d41\u0d1c\u0d28\u0d41\u0d32\u0d15\u0d4d(\u0d0e)\u0d32\u0d4d\u0d32\u0d28\u0d4d(\u0d0f) \u0d35\u0d47\u0d33 <br/> \u0d38\u0d28\u0d4d\u0d24\u0d4b\u0d37\u0d2e\u0d41\u0d32\u0d4d(\u0d12)\u0d38\u0d17\u0d47\u0d35\u0d4d(\u0d05)\u0d28\u0d3f \u0d28\u0d40\u0d35\u0d41 <br/> \u0d2e\u0d28\u0d4b\u0d30\u0d25 \u0d2b\u0d32 \u0d26\u0d3e\u0d2f\u0d3f\u0d28\u0d3f\u0d35\u0d4d(\u0d05)\u0d28\u0d3f <br/> \u0d15\u0d3e\u0d28\u0d4d\u0d24\u0d2e\u0d17\u0d41 \u0d2a\u0d47\u0d30\u0d41 \u0d2a\u0d4a\u0d28\u0d4d\u0d26\u0d3f\u0d24\u0d3f\u0d35\u0d3f <br/> \u0d15\u0d3e\u0d30\u0d41\u0d23\u0d4d\u0d2f \u0d2e\u0d42\u0d30\u0d4d\u0d24\u0d3f\u0d35\u0d48 \u0d1c\u0d17\u0d2e\u0d41 <br/> \u0d15\u0d3e\u0d2a\u0d3e\u0d21\u0d3f\u0d28 \u0d24\u0d32\u0d4d\u0d32\u0d3f \u0d17\u0d26\u0d3e \u0d28\u0d47\u0d28\u0d41 <br/> \u0d28\u0d40\u0d26\u0d41 \u0d2c\u0d3f\u0d21\u0d4d\u0d21\u0d28\u0d41 \u0d32\u0d3e\u0d32\u0d3f\u0d28\u0d4d\u0d1a\u0d3f (\u0d2a\u0d3e\u0d32\u0d3f\u0d28\u0d4d\u0d1a\u0d41)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d08 \u0d2e\u0d42\u0d30\u0d4d\u0d24\u0d3f(\u0d2f\u0d4d)\u0d07\u0d28\u0d4d\u0d24 \u0d24\u0d47\u0d1c\u0d4b-\u0d2e\u0d2f\u0d2e\u0d48- <br/> (\u0d2f\u0d4d)\u0d07\u0d1f\u0d41 \u0d35\u0d32\u0d46 \u0d15\u0d40\u0d30\u0d4d\u0d24\u0d3f \u0d35\u0d3f\u0d38\u0d4d\u0d2b\u0d42\u0d30\u0d4d\u0d24\u0d3f(\u0d28\u0d4d)- <br/> \u0d07\u0d1f\u0d4d\u0d32\u0d28\u0d41 \u0d17\u0d41\u0d23 \u0d2e\u0d42\u0d30\u0d4d\u0d24\u0d3f \u0d24\u0d4d\u0d30\u0d3f-\u0d32\u0d4b\u0d15\u0d2e\u0d41\u0d32\u0d4b <br/> \u0d1c\u0d42\u0d1a\u0d3f\u0d28(\u0d2f\u0d4d)\u0d0e\u0d28\u0d4d\u0d26\u0d4d(\u0d10)\u0d28 \u0d17\u0d32\u0d26\u0d3e <br/> \u0d0f\u0d2e\u0d4b \u0d24\u0d4a\u0d32\u0d3f \u0d28\u0d4b\u0d2e\u0d41 \u0d28\u0d4b\u0d1a\u0d3f\u0d24\u0d3f\u0d28\u0d4b <br/> \u0d28\u0d40 \u0d2a\u0d3e\u0d26 \u0d2a\u0d26\u0d4d\u0d2e \u0d26\u0d30\u0d4d\u0d36\u0d28\u0d2e\u0d41 <br/> \u0d35\u0d47\u0d2e\u0d3e\u0d30\u0d41 \u0d32\u0d2d\u0d3f\u0d28\u0d4d\u0d1a\u0d3f \u0d15\u0d43\u0d24\u0d4d(\u0d06)\u0d30\u0d4d\u0d25\u0d41\u0d21\u0d28\u0d4d(\u0d10)\u0d24\u0d3f <br/> \u0d28\u0d3e \u0d2e\u0d28\u0d35\u0d3f\u0d28\u0d4d(\u0d06)\u0d32\u0d15\u0d3f\u0d28\u0d4d\u0d1a\u0d3f (\u0d2a\u0d3e\u0d32\u0d3f\u0d28\u0d4d\u0d1a\u0d41)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d30\u0d3e\u0d1c\u0d4d(\u0d06)\u0d27\u0d3f \u0d30\u0d3e\u0d1c\u0d28\u0d4d-\u0d2e\u0d15\u0d41\u0d1f\u0d40 <br/> \u0d24\u0d1f \u0d2e\u0d23\u0d3f \u0d30\u0d3e\u0d1c \u0d2a\u0d3e\u0d26\u0d3e <br/> \u0d28\u0d47 \u0d1a\u0d3e\u0d32 \u0d28\u0d3f\u0d1c \u0d38\u0d28\u0d4d\u0d28\u0d3f\u0d27\u0d3f\u0d28\u0d3f \u0d15\u0d4b\u0d30\u0d3f <br/> \u0d38\u0d2e\u0d38\u0d4d\u0d24 \u0d1c\u0d28\u0d41\u0d32\u0d15\u0d4d(\u0d0e)\u0d32\u0d4d\u0d32 \u0d35\u0d30\u0d26\u0d3e <br/> \u0d30\u0d3e\u0d1c \u0d2e\u0d41\u0d16\u0d40 \u0d36\u0d4d\u0d2f\u0d3e\u0d2e \u0d15\u0d43\u0d37\u0d4d\u0d23 \u0d28\u0d41\u0d24\u0d3e <br/> \u0d15\u0d3e\u0d28\u0d4d\u0d1a\u0d40 \u0d2a\u0d41\u0d30\u0d4d(\u0d0f)\u0d36\u0d4d\u0d35\u0d30\u0d40 \u0d35\u0d3f\u0d15\u0d38 <br/> \u0d30\u0d3e\u0d1c\u0d40\u0d35 \u0d26\u0d33\u0d4d(\u0d06)\u0d15\u0d4d\u0d37\u0d40 \u0d1c\u0d17\u0d24\u0d4d-\u0d38\u0d3e\u0d15\u0d4d\u0d37\u0d40 <br/> \u0d13 \u0d2a\u0d4d\u0d30\u0d38\u0d28\u0d4d\u0d28 \u0d2a\u0d30\u0d3e \u0d36\u0d15\u0d4d\u0d24\u0d40 (\u0d2a\u0d3e\u0d32\u0d3f\u0d28\u0d4d\u0d1a\u0d41)"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d15\u0d28\u0d15 \u0d17\u0d3f\u0d30\u0d3f \u0d38\u0d26\u0d28 \u0d32\u0d32\u0d3f\u0d24 \u0d28\u0d3f\u0d28\u0d41 \u0d2d\u0d1c\u0d28 <br/> \u0d38\u0d28\u0d4d\u0d24\u0d24\u0d2e\u0d41 \u0d38\u0d47\u0d2f\u0d28\u0d3f \u0d1c\u0d21\u0d41\u0d21\u0d28\u0d41 <br/> \u0d35\u0d3f\u0d28\u0d41\u0d2e\u0d41 \u0d28\u0d3f\u0d16\u0d3f\u0d32 \u0d2d\u0d41\u0d35\u0d28 \u0d1c\u0d28\u0d28\u0d3f\u0d35\u0d3f(\u0d2f\u0d4d)\u0d07\u0d2a\u0d41\u0d21\u0d41 <br/> \u0d2e\u0d3e \u0d26\u0d41\u0d30\u0d3f\u0d24\u0d2e\u0d41 \u0d26\u0d40\u0d30\u0d4d\u0d1a\u0d3f \u0d35\u0d30\u0d3e\u0d32\u0d4d(\u0d07)\u0d1a\u0d4d\u0d1a\u0d3f (\u0d2a\u0d3e\u0d32\u0d3f\u0d28\u0d4d\u0d1a\u0d41)"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092a\u093e\u0932\u093f\u0928\u094d\u091a\u0941 \u0915\u093e\u092e\u093e\u0915\u094d\u0937\u0940 \u092a\u093e\u0935\u0928\u0940 <br/> \u092a\u093e\u092a \u0936\u092e\u0928\u0940 \u0905\u092e\u094d\u092c"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u091a\u093e\u0932\u093e \u092c\u0939\u0941 \u0935\u093f\u0927\u092e\u0941\u0917\u093e \u0928\u093f\u0928\u094d\u0928\u0941 <br/> \u0938\u0926\u093e \u0935\u0947\u0921\u0941\u0915\u094b\u0928\u0947\u0921\u093f \u0928\u093e\u092f\u0928\u094d\u0926\u094d(\u090f)\u0932 <br/> \u0908 \u0932\u093e\u0917\u0941 \u091c\u0947\u0938\u0947\u0935\u0941 \u0935\u0947\u0924 <br/> \u0939\u0930\u093f\u0928\u094d\u091a\u0935\u0947 \u0935\u0947\u0917\u092e\u0947 \u0928\u0928\u094d\u0928\u0941 (\u092a\u093e\u0932\u093f\u0928\u094d\u091a\u0941)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u0938\u094d\u0935\u093e\u0928\u094d\u0924\u092e\u094d\u092c\u0941\u0932\u094b\u0928 \u0928\u093f\u0928\u094d\u0928\u0947 <br/> \u0926\u0932\u091a\u093f\u0928 \u0938\u0941\u091c\u0928\u0941\u0932\u0915\u094d(\u090f)\u0932\u094d\u0932\u0928\u094d(\u090f) \u0935\u0947\u0932 <br/> \u0938\u0928\u094d\u0924\u094b\u0937\u092e\u0941\u0932\u094d(\u0913)\u0938\u0917\u0947\u0935\u094d(\u0905)\u0928\u093f \u0928\u0940\u0935\u0941 <br/> \u092e\u0928\u094b\u0930\u0925 \u092b\u0932 \u0926\u093e\u092f\u093f\u0928\u093f\u0935\u094d(\u0905)\u0928\u093f <br/> \u0915\u093e\u0928\u094d\u0924\u092e\u0917\u0941 \u092a\u0947\u0930\u0941 \u092a\u094b\u0928\u094d\u0926\u093f\u0924\u093f\u0935\u093f <br/> \u0915\u093e\u0930\u0941\u0923\u094d\u092f \u092e\u0942\u0930\u094d\u0924\u093f\u0935\u0948 \u091c\u0917\u092e\u0941 <